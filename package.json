{
  "name": "astrid-task-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node scripts/build-with-migrations.js",
    "build:next": "next build",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:post-deploy": "node scripts/post-deploy-migrate.js",
    "db:reset": "tsx scripts/reset-database.ts",
    "db:reset:unsafe": "prisma db push --force-reset",
    "db:safety-check": "tsx scripts/db-safety-check.ts",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:test": "node -e \"const { PrismaClient } = require('@prisma/client'); const prisma = new PrismaClient(); prisma.$connect().then(() => console.log('✅ Database connected')).catch(e => console.error('❌ Database failed:', e)).finally(() => prisma.$disconnect())\"",
    "db:setup-production": "tsx scripts/setup-production-db.ts",
    "db:deploy-production": "tsx scripts/deploy-production-migration.js",
    "db:push-production": "tsx scripts/deploy-production-migration.js",
    "db:verify-production": "tsx scripts/verify-production.js",
    "email:test": "tsx scripts/test-email.ts",
    "email:config": "tsx scripts/test-email-config.ts",
    "test:verification": "tsx scripts/test-verification-logic.ts",
    "test:auth": "tsx scripts/test-auth.ts",
    "test:rate-limit": "vitest tests/auth/rate-limit-simple.test.ts",
    "predeploy:check": "tsx scripts/predeploy-check.ts",
    "check:model-sync": "tsx scripts/check-model-sync.ts",
    "check:api-breaking": "tsx scripts/check-api-breaking-changes.ts",
    "predeploy": "tsx scripts/predeploy-self-healing.ts",
    "predeploy:simple": "npm run check:model-sync && npm run check:api-breaking && npm run test:run && npx tsc --noEmit && npm run lint && echo 'All predeploy checks passed!'",
    "predeploy:quick": "npm run check:model-sync && npm run check:api-breaking && npx tsc --noEmit && npm run lint && echo 'Quick checks passed'",
    "validate:env": "tsx scripts/validate-production-env.ts",
    "validate:env:production": "NODE_ENV=production tsx scripts/validate-production-env.ts",
    "predeploy:build": "npm run build && echo 'Build test passed'",
    "predeploy:essential": "npm run predeploy:quick && npm run predeploy:build",
    "predeploy:full": "npm run test:run && npm run test:e2e && npm run predeploy:essential && echo 'Full predeploy with E2E passed!'",
    "predeploy:with-e2e": "npm run predeploy:full",
    "predeploy:no-e2e": "npm run predeploy",
    "fix:deployment": "tsx scripts/fix-deployment-issues.ts",
    "monitor:vercel": "tsx scripts/monitor-vercel-logs.ts",
    "monitor:vercel:watch": "tsx scripts/monitor-vercel-logs.ts --watch",
    "monitor:vercel:no-fix": "tsx scripts/monitor-vercel-logs.ts --no-fix",
    "db:optimize": "tsx scripts/optimize-database.ts",
    "db:health": "tsx -e \"import { PerformanceMonitoring } from './lib/database-utils'; PerformanceMonitoring.healthCheck().then(r => console.log(JSON.stringify(r, null, 2)))\"",
    "db:migrate-production": "tsx scripts/migrate-production.ts",
    "deploy": "tsx scripts/deploy-to-vercel.ts",
    "deploy:production": "tsx scripts/deploy-production.ts",
    "deploy:staging": "tsx scripts/deploy-production.ts --staging",
    "deploy:trigger": "tsx scripts/trigger-workflow.ts",
    "setup:github-secrets": "./scripts/setup-github-secrets.sh",
    "dev": "next dev -H 0.0.0.0",
    "dev:local": "node scripts/dev-local.js",
    "docs:review": "tsx scripts/review-docs-after-changes.ts",
    "lint": "eslint . --cache",
    "typecheck": "tsc --noEmit",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ai-agent": "tsx scripts/quick-test-ai-agent.ts",
    "test:ai-agent:full": "tsx scripts/test-ai-agent-workflow-local.ts",
    "fix:ai-agent-webhook": "tsx scripts/fix-ai-agent-webhook-url.ts",
    "check:production-ready": "tsx scripts/pre-production-ai-agent-checklist.ts",
    "generate-placeholder-images": "node scripts/generate-placeholder-images.js",
    "build:mcp:oauth": "cd mcp && node build-mcp-oauth.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install --with-deps",
    "playwright:setup": "tsx --env-file=.env.local scripts/create-test-user.ts",
    "test:suggest": "tsx scripts/suggest-test-type.ts",
    "offline:check": "tsx scripts/check-offline-mutations.ts",
    "offline:retry": "tsx scripts/retry-failed-mutations.ts",
    "offline:clear": "tsx scripts/clear-mutations.ts",
    "validate:settings": "tsx .claude/validate-settings.ts",
    "validate:settings:fix": "tsx .claude/validate-settings.ts --fix",
    "validate:mcp:oauth": "tsx scripts/validate-mcp-oauth.ts",
    "test:caching": "tsx --env-file=.env.local scripts/test-progressive-caching.ts",
    "monitor:cache": "./scripts/monitor-cache-usage.sh",
    "work:start": "tsx scripts/worktree-start.ts",
    "work:done": "tsx scripts/worktree-cleanup.ts",
    "work:list": "tsx scripts/worktree-list.ts",
    "test:oauth": "tsx --env-file=.env.local scripts/test-oauth-local.ts",
    "setup:ios-oauth": "tsx --env-file=.env.local scripts/setup-ios-oauth.ts",
    "setup:astrid-config": "tsx scripts/setup-astrid-config.ts",
    "setup:astrid-config:force": "tsx scripts/setup-astrid-config.ts --force",
    "predeploy:dry": "tsx scripts/predeploy-self-healing.ts --dry",
    "predeploy:ci": "tsx scripts/predeploy-self-healing.ts --ci",
    "deploy:canary": "tsx scripts/deploy-canary.ts",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --cache",
      "bash -c 'npx tsc --noEmit'"
    ],
    "*.{js,jsx}": [
      "eslint --fix --cache"
    ]
  },
